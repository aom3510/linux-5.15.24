&adc {
	/* ANA0, ANA1 are dedicated pins and don't need pinctrl: only in6. */
	pinctrl-names = "default";
	pinctrl-0 = <&adc12_ain_pins_a>;		/* PC3 (ADC1_IN13), PF12 (ADC1_IN6), PF13 (ADC2_IN2), PF14 (ADC2_IN6),
	vdd-supply = <&vdd>;
	vdda-supply = <&vdda>;				/* Supply vdda pin by using a PMIC regulator
	vref-supply = <&vdda>;				/* Use VREFBUF (It needs to be enabled as well) */
	status = "okay";

	adc1: adc@0 {
		st,adc-channels = <0 1 6>;
		st,min-sample-time-nsecs = <1000>;
		status = "okay";
	};

	adc2: adc@100 {
		st,adc-channels = <0 1 2>;
		st,min-sample-time-nsecs = <1000>;
		status = "okay";
	};
};

&spi5 {
    pinctrl-names = "default", "sleep";
